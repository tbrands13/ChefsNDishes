<div class="d-flex justify-content-around mb-6">
    <h1 class="text-center">Chefs | <a href="dishes">Dishes</a></h1>
    <a href="addChef" class="btn btn-lg btn-outline-info">Add a Chef</a>
</div>
<div class="mt-4">
    <h3 class="text-decoration-underline">Check out our roster of Chefs!</h3>
</div>
<div>
    <table class="table table-striped table-hover">
        <tr>
            <th>Name</th>
            <th>Birthday</th>
            <th># of Dishes</th>
        </tr>
        @foreach (Chef c in ViewBag.AllChefs)
        {
        <tr>
            <td>@c.ChefFirstName, @c.ChefLastName</td>
            <td>@c.DOB</td>
            <td>@c.CreatedDishes.Count</td>
        </tr>
        }
    </table>
</div>